#!/bin/zsh
# Binds

bindkey "^[[1;5C" forward-word
bindkey "^[[1;5D" backward-word
## Binds tsotd to CTRL+t
bindkey -s '^a' 'tsotd^M'
## Binds codium to CTRL+o (only in a display server)
bindkey -s '^o' '$EDGUI .^M'
## You get the idea...
bindkey -s '^n' 'newfile^M'
bindkey -s '^P' 'toothfetch^M'
bindkey '^R' history-incremental-search-backward
